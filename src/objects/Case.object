    <validationRules>
        <fullName>Required_fields_when_closing_a_case</fullName>
        <active>true</active>
        <description>Makes certain fields required when closing cases</description>
        <errorConditionFormula>AND(
ISPICKVAL(Status, &quot;Completed&quot;) || ISPICKVAL(Status, &quot;Closed&quot;),
OR(
ISBLANK( Status_Notes__c ),
ISBLANK( Actual_LOE__c )
))</errorConditionFormula>
        <errorMessage>Please fill out the Closed date and/or Status Notes and/or Actual LOE section in order to successfully close this case.</errorMessage>
    </validationRules>
